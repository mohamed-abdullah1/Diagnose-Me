{
    "GlobalConfiguration": {
        "BaseUrl": "https://localhost:7199",
        "RATE_LIMITING_ENABLED": "true",
        "RateLimitOptions": {
            "EnableRateLimiting": true,
            "EnableEndpointRateLimiting": true,
            "EnableClientRateLimiting": true,
            "Period": "60s",
            "PeriodTimespan": 60,
            "Limit": 20,
            "DisableRateLimitHeaders": false,
            "ClientWhitelist": [
                "0.0.0.0"
            ]
        }
    }
}